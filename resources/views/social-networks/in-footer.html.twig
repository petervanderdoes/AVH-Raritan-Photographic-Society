<script>
    rpssocial.setapiconfig([
        {% for key,network in networks %}
        {% if network.api != false %}

        {
            'site': {{ key }},
            'success': {{ value.api }}
        },
        {% else %}
        {'site': {{ key }}},
        {% endif %}
        {% endfor %}
    ]);
    rpssocial.share.setconfig(rps_shareconfig);
</script>