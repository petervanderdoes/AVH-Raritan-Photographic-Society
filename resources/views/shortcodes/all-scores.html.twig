<div class="fix">
    <h4>{{ data.entries.cp.title }}</h4>
    <table>
        <tbody>
        <tr>
            <th class="form_frame_header" colspan="2">&nbsp;</th>
            {% for data in data.heading %}
                <th class="form_frame_header" colspan="{{ data.entries }}">{{ data.date }}</th>
            {% endfor %}
            <th class="form_frame_header">&nbsp;</th>
        </tr>
        <tr>
            <th class="form_frame_header">Member</th>
            <th class="form_frame_header">Cl.</th>
            {% for data in data.heading %}
                {% for i in 1..data.entries %}
                    <th style="width: 2rem" class="form_frame_header">{{ i }}</th>
                {% endfor %}
            {% endfor %}
            <th class="form_frame_header">Avg</th>
        </tr>
        {% for entry in data.entries.cp.users %}
            <tr>
                <td class="even_row" align="left">{{ entry.name }}</td>
                <td class="even_row" align="center">{{ entry.classification }}</td>
                {% for dates in entry.scores %}
                    {% for score in dates %}
                        <td class="even_row" align="center">{{ score.score }}<sup>{{ score.award }}</sup></td>
                    {% endfor %}
                {% endfor %}
                <td class="even_row" align="center">{{ entry.average_score|number_format(2, '.', ',') }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
