<p class="competition-theme first-para">The {{ count_entries }} entries submitted to Raritan Photographic Society for the theme "{{ theme_name }}" held on {{ date_text }}</p>
{% include 'inc/month_and_season_select_form.twig' %}
{% if images %}
    <div id="gallery-month-entries" class="gallery gallery-masonry gallery-columns-5">
        <div class="grid-sizer" style="width: 193px"></div>
        <div id="images"></div>
        {% for image in images %}
            {% include 'inc/photo_masonry.twig' %}
        {% endfor %}
    </div>
{% endif %}